{
  "name": "bodyworkplans",
  "version": "1.0.0",
  "description": "Small app with full body workout programs",
  "browser": "dist/bundle.js",
  "root": "/bodyworkplans/",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "postcss -o dist/bundle.css style.css",
    "build": "rollup -c"
  },
  "keywords": [
    "sport",
    "fitness",
    "bodyweight",
    "workout",
    "calisthenics"
  ],
  "author": "Simon Dassow",
  "repository": {
    "type": "git",
    "url": "https://github.com/sdassow/bodyworkplans.git"
  },
  "license": "MIT",
  "dependencies": {
    "@riotjs/observable": "^4.0.4",
    "@riotjs/route": "^6.0.2",
    "date-fns": "^2.16.1",
    "riot": "^4.14.0"
  },
  "devDependencies": {
    "@riotjs/compiler": "^4.11.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "cssnano": "^4.1.10",
    "eslint-plugin-json": "^2.1.2",
    "normalize.css": "^8.0.1",
    "postcss": "^8.1.1",
    "postcss-import": "^12.0.1",
    "rollup": "^2.26.11",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-manifest-json": "^1.4.2",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-riot": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^5.2.1",
    "spectre.css": "^0.5.9"
  },
  "files": [
    "dist/bundle.js"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "ENV": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:json/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    },
    "ignorePatterns": [
      "node_modules/"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "eslintIgnore": [
    "*.riot",
    "*.css"
  ]
}
